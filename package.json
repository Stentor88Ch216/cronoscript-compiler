{
	"scripts": {
		"build": "tsc",
		"makeTsParser": "antlr4ts -Dlanguage=TypeScript CronoScript.g4 -o ./antlr/antlr4ts",
		"makeJavaParser": "cd ./antlr/scripts && ./makeJavaParser.sh",
		"grun": "cd ./antlr/scripts && ./runGrun.sh",
		"test": "pnpm run makeJavaParser && pnpm run grun",
		"start": "node dist/app.js",
		"dev": "pnpm run build && pnpm run start"
	},
	"devDependencies": {
		"antlr4ts-cli": "0.5.0-alpha.4",
		"nodemon": "^3.0.2",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"antlr4ts": "0.5.0-alpha.4"
	}
}
